{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Venue Registration",
    "type": "object",
    "properties": {
      "venueID": {
        "type": "string"
      },
      "venueName": {
        "type": "string"
      },
      "email": {
        "type": "string",
        "format": "email"
      },
      "location": {
        "type": "string"
      },
      "password": { 
        "type": "string",
        "minLength": 6
      }
    },
    "required": [
      "venueName",
      "email",
      "password"
    ]
  }
  